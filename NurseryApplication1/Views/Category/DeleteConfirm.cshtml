@model NurseryApplication1.Models.CategoryDto
@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>DeleteConfirm</h2>


<h2>Are you sure you want to delete this Category?</h2>
<a href="/Category/List">Cancel</a>
<h2>@Model.CategoryName</h2>

<form action="/Category/Delete/@Model.CategoryId" method="POST">
    <input type="submit" value="Confirm Delete" />

</form>